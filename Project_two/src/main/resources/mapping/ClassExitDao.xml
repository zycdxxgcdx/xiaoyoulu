<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zy.final_project.dao.ClassExitDao">
    <insert id="delByClass" parameterType="com.zy.final_project.entity.message.MessageExitClass">
        insert into classexit(userId,classId,exitTime,exitType)
        values(#{userId},#{classId},#{time},#{exitType})
    </insert>
    <insert id="exitClass" parameterType="com.zy.final_project.entity.message.MessageExitClass">
        insert into classexit(userId,classId,exitTime,exitType)
        values(#{userId},#{classId},#{time},#{exitType})
    </insert>
</mapper>